
function test_system:
    trigger:
        set system property "blob" to "hello"
        set {var} to system property "blob"
        assert {var} is "hello"
        delete system property "blob"

function my_function:
    trigger:
        print "hello there"
        wait 0 seconds
        print "general kenobi"
        print 66

function test_method:
    trigger:
        assert java version is 61
        print the java version
        print null
        print "hello"
        stop
        print "never printed"

function testing_variable:
    trigger:
        set {test} to "variable value"
        assert {test} is set
        assert {test} is "variable value"
        assert none is none
        assert false = false
        assert true is not false
        print {test}

function testing_map:
    trigger:
        print {bean}
        set {test} to a new map
        assert {test} exists
        set "name" in {test} to "Jeremy"
        print "name" in {test}
        set "thing" in {test} to "name" in {test}
        assert "thing" in {test} is "Jeremy"
        set ("thing" in {test}) to ("Frog")
        assert ("thing" in {test}) is "Frog"
        print "thing" in {test}
        return {test}

function to_be_run:
    trigger:
        print "was run"
        return new map

function run_function:
    trigger:
        print "call function"
        assert "hello" is a string
        set {map} to to_be_run()
        set "key" in {map} to "Blob"
        print "key" in {map}
        return "bees"
