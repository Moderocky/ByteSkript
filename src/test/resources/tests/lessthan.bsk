
function test:
    trigger:
        assert 3 < 4: "Less than greater check failed."
        assert (4 < 4) is false: "Less than self check failed."
        assert (5 < 4) is false: "Less than smaller check failed."
        assert -2 < 5: "Less than negative check failed."
        assert -3 < -2: "Less than negative check failed."
        return true
