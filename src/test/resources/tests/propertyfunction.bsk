
type Blob:
    function test:
        return: string
        trigger:
            return "hello"

function test:
    trigger:
        set {var} to a new blob
        assert {var} exists
        assert {var} is a blob
        assert test() from {var} is "hello"
        set {x} to test() from {var}
        assert {x} is "hello"
        return true
